<MainScreen>: 
    MDBoxLayout: 
        orientation: "vertical"
        md_bg_color: app.theme_cls.bg_normal   
        MDTopAppBar: 
            title: 'Facebook'
            height: dp(60)
            elevation: dp(5)
            anchor_title: 'left'
            right_action_items: [['plus', lambda x: x],['account-search', lambda x: x], ['message', lambda x: app.switch_screen('message_screen')]]            
            margin: 0
                       

        MDTabs:
            size: dp(0.5), dp(.1)


            HomeTab:
                icon: 'home'
                md_bg_color: 'lightgrey'
                
                MDSeparator:
                    md_bg_color: 'gray'


                ScrollView:
                    do_scroll_y: True
                    effect_cls: 'ScrollEffect'
                    bar_width: 0
                    MDBoxLayout:
                        adaptive_height: True
                        orientation: 'vertical'
                        spacing: dp(20)
                        MDBoxLayout:
                            height: dp(150)
                            size_hint_y: None
                            ScrollView:
                                do_scroll_x: True
                                do_scroll_y: False
                                bar_width: 0
                                MDBoxLayout: 
                                    id: story_layout
                                    orientation: 'horizontal'
                                    adaptive_width: True
                                    spacing: dp(10)
                                    padding: [10, 0, 0, 0]
                                    StoryWithIcon:

                        MDBoxLayout:
                            orientation: 'vertical'
                            md_bg_color: app.theme_cls.bg_normal
                            size_hint_y: None
                            adaptive_height: True
                            MDList:
                                id: company
                                spacing: dp(5)
                                MDCard:
                                    size_hint: .5, .3
                                    pos_hint: {"center_x": .5, "center_y": .5}

            FriendsTap:
                icon: 'account-multiple'
                md_bg_color: 'lightgrey'  
                MDBoxLayout:
                    orientation: 'vertical'
                    spacing: dp(10) 

                    MDTopAppBar: 
                        height: dp(50)
                        title: "[color=#000000]Friends[/color]"
                        bold: True            
                        text_size: self.size
                        font_size: '15sp'
                        markup: True
                        anchor_title: 'left'
                        right_action_items: [['search-web', lambda x: x]]    
                        margin: 0
                        md_bg_color: 'lightgrey'
                        elevation: 0

                        MDRoundFlatButton:
                            text: 'Your Friends'
                            line_color: 'lightgray'
                            md_bg_color: 'gray'
                            text_color: 'black'
                            size_hint_x: dp(0.3)        
                   

                    MDBoxLayout:
                        orientation: 'horizontal'
                        size: dp(1), dp(15)
                        size_hint: (1, None)
                        spacing: dp(2)
                        padding: dp(5)
                    

                        MDLabel:
                            markup: True
                            text: "Friend requests [color=#ff0000ff]420[/color]"    
                            md_bg_color: 'lightgrey' 
                            font_size: '13sp'       

                        Widget:                          

                        MDLabel:
                            text: "see all"
                            halign: 'right'
                            theme_text_color: "Custom"
                            text_color: 'blue'
                            md_bg_color: 'lightgrey'  
                            font_size: '13sp'  

                    ScrollView:
                        do_scroll_y: True
                        effect_cls: 'ScrollEffect'
                        bar_width: 0
                        MDBoxLayout:
                            adaptive_height: True
                            orientation: 'vertical'
                            md_bg_color: app.theme_cls.primary_color                                                        

                            MDBoxLayout:
                                orientation: 'vertical'
                                md_bg_color: app.theme_cls.bg_normal
                                size_hint_y: None
                                adaptive_height: True
                                MDList: 
                                    id: friends_layout
                                    spacing: dp(5)


            VideoTab:
                icon: 'video'
                md_bg_color: 'lightgrey'  
                pos_hint: {"center_x": .5, "center_y": .5}

                MDBoxLayout:
                    orientation: 'vertical'
                    spacing: dp(10)
                    MDTopAppBar: 
                        height: dp(50)
                        title: "[color=#000000]Video[/color]"
                        bold: True            
                        text_size: self.size
                        font_size: '15sp'
                        markup: True
                        anchor_title: 'left'
                        right_action_items: [['account', lambda x: x], ['search-web', lambda x: x]]                          
                        margin: 0
                        md_bg_color: 'lightgrey'
                        elevation: 0  

                        MDGridLayout:
                            cols: 7

                            MDRoundFlatButton:
                                text: 'For you'
                                line_color: 'lightgray'
                                md_bg_color: 'gray'
                                text_color: 'black'

                            MDRoundFlatButton:
                                text: 'Live'
                                line_color: 'lightgray'
                                md_bg_color: 'gray'
                                text_color: 'black'

                            MDRoundFlatButton:
                                text: 'Reels'
                                line_color: 'lightgray'
                                md_bg_color: 'gray'
                                text_color: 'black'

                            MDRoundFlatButton:
                                text: 'Following'
                                line_color: 'lightgray'
                                md_bg_color: 'gray'
                                text_color: 'black'

                    MDSeparator:
                        md_bg_color: 'gray'



                    ScrollView:
                        do_scroll_y: True
                        effect_cls: 'ScrollEffect'
                        bar_width: 0
                        MDBoxLayout:
                            adaptive_height: True
                            orientation: 'vertical'
                            spacing: dp(20)


                           
                            MDBoxLayout:
                                orientation: 'vertical'
                                md_bg_color: app.theme_cls.bg_normal
                                size_hint_y: None
                                adaptive_height: True
                                MDList:
                                    id: company_video
                                    spacing: dp(5)
                                    MDCard:
                                        size_hint: dp(.5), dp(.3)
                                        pos_hint: {"center_x": .5, "center_y": .5}       

            MarketPlaceTab:
                icon: 'store'
                md_bg_color: 'lightgrey'            


                MDBoxLayout:
                    orientation: 'vertical'
                    spacing: dp(10)
                    MDTopAppBar: 
                        height: dp(50)
                        title: "[color=#000000]Marketplace[/color]"
                        bold: True            
                        text_size: self.size
                        font_size: '15sp'
                        markup: True
                        anchor_title: 'left'
                        right_action_items: [['account', lambda x: x], ['search-web', lambda x: x]]                          
                        margin: 0
                        md_bg_color: 'lightgrey'
                        elevation: 0  


                        MDGridLayout:
                            cols: 2

                            MDRoundFlatIconButton:
                                text: 'Sell'
                                line_color: 'lightgray'
                                md_bg_color: 'gray'
                                text_color: 'black'
                                size_hint_x: dp(0.1) 
                                icon: 'book-check'   

                            MDRoundFlatIconButton:
                                text: 'Cateories'
                                line_color: 'lightgray'
                                md_bg_color: 'gray'
                                text_color: 'black'
                                size_hint_x: dp(0.1)    
                                icon:'menu'                                

                    MDSeparator:
                        md_bg_color: 'gray'


                    ScrollView:
                        do_scroll_y: True
                        effect_cls: 'ScrollEffect'
                        bar_width: 0
                        MDBoxLayout:
                            adaptive_height: True
                            orientation: 'vertical'
                            spacing: dp(20)

                            MDBoxLayout:
                                orientation: 'horizontal'
                                spacing: dp(2)
                                padding: dp(10)

                                MDLabel:
                                    markup: True
                                    text: "Todays Picks"  

                                MDLabel:
                                    text: "Change"
                                    halign: "right"
                                    icon: 'location-enter'
                                    theme_text_color: "Custom"
                                    text_color: 'blue'

                            MDGridLayout:
                                id: company_market_place_sales
                                cols: 2
                                spacing: "12dp"
                                adaptive_height: True

                        
            NotificationTab:
                icon: 'bell'
                md_bg_color: 'lightgrey'  
                badge_icon: 'numeric-10'
                pos_hint: {"center_x": .5, "center_y": .5}
                badge_icon_color: 0, 0, 1, 1
                badge_font_size: "24sp"

                MDBoxLayout:
                    orientation: 'vertical'
                    spacing: dp(10)
                    MDTopAppBar: 
                        height: dp(50)
                        title: "[color=#000000]Notifications[/color]"
                        bold: True            
                        text_size: self.size
                        font_size: '15sp'
                        markup: True
                        anchor_title: 'left'
                        right_action_items: [['search-web', lambda x: x]]    
                        margin: 0
                        md_bg_color: 'lightgrey'
                        elevation: 0                    

                    MDSeparator:
                        md_bg_color: 'gray'



                    ScrollView:
                        do_scroll_y: True
                        effect_cls: 'ScrollEffect'
                        bar_width: 0
                        MDBoxLayout:
                            adaptive_height: True
                            orientation: 'vertical'
                            md_bg_color: app.theme_cls.primary_color           
                            MDBoxLayout:
                                orientation: 'horizontal'
                                size_hint: (None, None)
                                size: dp(600), dp(40)
                                spacing: dp(2)
                                MDLabel:
                                    markup: True
                                    text: "  [color=#000000]New[/color]"  

                            MDBoxLayout:
                                orientation: 'vertical'
                                md_bg_color: app.theme_cls.bg_normal
                                size_hint_y: None
                                adaptive_height: True
                                MDList: 
                                    id: notifications_layout
                                    spacing: dp(5)
                   

            MenuTab:
                icon: 'menu'
                md_bg_color: 'lightgrey' 

                MenuTabScreen:



