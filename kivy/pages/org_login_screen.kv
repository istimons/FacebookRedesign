<OrganizationLogInScreen>:
    MDBoxLayout: 
        orientation: "vertical"
        md_bg_color: app.theme_cls.bg_normal   
        size_hint: dp(1), dp(1)

        MDTopAppBar: 
            height: dp(50)
            pos_hint: {"top": 1}
            title: "Feedback"
            text_size: self.size
            font_size: '15sp'
            markup: True
            anchor_title: 'left'
            # left_action_items: [['camera', lambda x: x],['account-search', lambda x: x], ['dots-vertical', lambda x: x]]            
            margin: 0

        MDBoxLayout:
            orientation: 'vertical'
            size_hint: (1, None)
            size: dp(1700), dp(35)
            md_bg_color: 'lightgrey'

            MDLabel:
                text: "  Create company account"
                theme_text_color: "Custom"
                text_color: app.theme_cls.primary_color


        ScrollView:
            do_scroll_y: True
            effect_cls: 'ScrollEffect'
            bar_width: dp(2)
            size_hint: dp(1), dp(1)
            padding: dp(8),dp(8),dp(8),dp(8)
            # spacing: dp(20)
            pos_hint: {"center_x": .5, "center_y": .5}
            # size: dp(400), dp(950)
            md_bg_color: 'red'

            MDBoxLayout:
                orientation: 'vertical'
                size_hint_y: 1
                adaptive_height: True
                # md_bg_color: 'lightgrey'
                spacing: dp(20)

                NameContent:
                AddressContent:
                EmailContent:
                ContatContent:
                SocialContent:
                WhatsAppSocialContent:
                TwitterSocialContent:
                InstagramSocialContent:
                BlogORWebsite:

                MDRoundFlatButton:
                    text: 'Submit'
                    md_bg_color: 'teal'
                    theme_text_color: 'Custom'
                    text_color: 'white'
                    pos_hint: {"center_x": .5, "center_y": 0}
                    size_hint: (0.5, 0.2)
                    on_press:
                        app.switch_screen('main_screen')   

                MDSeparator:
                    line_color: 'white'
                    size_hint_y: dp(0.5)

   

<NameContent@MDBoxLayout>:    
    orientation: 'vertical'
    size_hint: dp(.95), (None)   
    size: dp(60), dp(100)             
    pos_hint: {"center_x": .5, "center_y": .8}           
    spacing: dp(2)
    line_color: 'gray'    
    radius: [dp(10), ]
    padding: 0, 0, 0, "26dp"   # for text alignment (spacing)
    md_bg_color: 'gray'

                    
    MDLabel:
        halign: 'left'
        valign: 'top'
        text:'  Name:'                          
        pos_hint: {"center_x": .5, "center_y": .5}    
        font_size: '15sp'
        size_hint: (1, 1)
        # md_bg_color: 'lightblue'                                

    MDTextField:                        
        max_height: "100dp"
        mode: "fill"
        multiline: True
        required: True
        fill_color:'lightblue'
        icon_left: 'library'
        hint_text: 'Company/Organization name'
        font_size: '15sp'
        helper_text: "Desired Company/Organization name"
        helper_text_mode: "persistent"           






